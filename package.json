{
  "name": "react-19",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "deploy:dev": "npm run build:dev",
    "deploy:prod": "npm run build:prod",
    "deploy:s3:dev": "npm run build:dev && aws s3 sync dist s3://$S3_BUCKET --delete --region $AWS_REGION",
    "deploy:s3:prod": "npm run build:prod && aws s3 sync dist s3://$S3_BUCKET --delete --region $AWS_REGION",
    "invalidate:cf": "aws cloudfront create-invalidation --distribution-id $CF_DISTRIBUTION_ID --paths '/*'",
    "deploy:cf:dev": "npm run deploy:s3:dev && npm run invalidate:cf",
    "deploy:cf:prod": "npm run deploy:s3:prod && npm run invalidate:cf",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "storybook": "storybook dev -p 6007 --no-open --ci",
    "build-storybook": "storybook build --disable-telemetry",
    "preview": "vite preview",
    "generate:images": "node scripts/generate-images.mjs",
    "check:images": "node scripts/check-image-usage.mjs",
    "generate:api": "swagger-typescript-api generate -p http://3.34.140.187:94/public-survey/v3/api-docs/ALL -o ./src/api -n test.ts   --extract-types --modular   --no-client"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.14",
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "sass": "^1.97.1",
    "simplebar": "^6.3.3",
    "simplebar-react": "^3.3.2",
    "swagger-typescript-api": "^13.2.16",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx-self": "^7.27.1",
    "@babel/plugin-transform-react-jsx-source": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@locator/babel-jsx": "^0.5.1",
    "@locator/runtime": "^0.5.1",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.97.2",
    "storybook": "^8.6.15",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-sass-dts": "^1.3.35",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
